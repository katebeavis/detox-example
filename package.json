{
  "name": "DetoxExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "detox": "^19.10.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/config.json",
    "behavior": {
      "init": {
        "reinstallApp": true,
        "exposeGlobals": true
      },
      "cleanup": {
        "shutdownDevice": false
      }
    },
    "skipLegacyWorkersInjection": true,
    "apps": {
      "ios.ci": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/DetoxExample.app",
        "build": "xcodebuild -workspace ios/DetoxExample.xcworkspace -scheme DetoxExample -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build"
      },
      "ios.debug": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/DetoxExample.app",
        "build": "xcodebuild -workspace ios/DetoxExample.xcworkspace -scheme DetoxExample -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build"
      },
      "android.ci": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/uat/app-uat-debug.apk",
        "testBinaryPath": "android/app/build/outputs/apk/androidTest/uat/debug/app-uat-debug-androidTest.apk",
        "build": "cd android && ./gradlew assembleUatDebug assembleAndroidTest -DtestBuildType=debug && cd .."
      },
      "android.debug": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/uat/app-uat-debug.apk",
        "testBinaryPath": "android/app/build/outputs/apk/androidTest/uat/debug/app-uat-debug-androidTest.apk",
        "build": "cd android && ./gradlew assembleUatDebug assembleAndroidTest -DtestBuildType=debug && cd .."
      }
    },
    "devices": {
      "simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 8"
        }
      },
      "emulator.debug": {
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_API_30"
        }
      },
      "emulator.ci": {
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_API_30"
        },
        "headless": true
      }
    },
    "configurations": {
      "ios.ci": {
        "device": "simulator",
        "app": "ios.ci"
      },
      "ios.debug": {
        "device": "simulator",
        "app": "ios.debug"
      },
      "android.ci": {
        "device": "emulator.ci",
        "app": "android.ci"
      },
      "android.debug": {
        "device": "emulator.debug",
        "app": "android.debug"
      }
    }
  }
}
